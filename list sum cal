 Program to compute the sum of integers in a list
def main():
    # Ask the user for input
    user_input = input("Enter integers separated by spaces: ")
    
    # Split the input string into a list of strings, then map to integers
    integer_list = list(map(int, user_input.split()))
    
    # Calculate the sum of the list
    total_sum = sum(integer_list)
    
    # Output the result
    print(f"The sum of the integers is: {total_sum}")

# Run the program
if __name__ == "__main__":
    main()
